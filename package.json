{
  "name": "angular-typescript-seed",
  "version": "1.1.0",
  "description": "Seed Project for Angular 1.5 w/ Typescript",
  "keywords": [
    "angular",
    "typescript",
    "angular material"
  ],
  "author": "Andrew Hamilton <andrew.hamilton.dev@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/andrew-hamilton-dev/angular-1.5-typescript/",
  "repository": {
    "type": "git",
    "url": "https://github.com/andrew-hamilton-dev/angular-1.5-typescript.git"
  },
  "bugs": {
    "url": "https://github.com/andrew-hamilton-dev/angular-1.5-typescript/issues"
  },
  "scripts": {
    "postinstall": ".\\node_modules\\.bin\\typings install && .\\node_modules\\.bin\\webdriver-manager update --standalone",
    "start": "npm run build",
    "build": "npm run dev:clean && npm run dev:copy && npm run docs:all && npm run web",
    "dev:clean": "rimraf builds && mkdir builds",
    "dev:copy": "xcopy /S .\\src\\assets\\*.* .\\builds\\assets /I",
    "web": "set DEV_ENV=true && opener http://localhost:8000/webpack-dev-server/ && node .\\webpack-server.js",
    "release": "npm run release:clean && npm run release:copy && npm run release:webpack",
    "release:clean": "rimraf dist && mkdir dist",
    "release:copy": "xcopy /S /I .\\src\\assets\\*.* .\\dist\\assets",
    "release:webpack": "set DEV_ENV=false && .\\node_modules\\.bin\\webpack --config .\\webpack-release.config.js -p --bail",
    "docs:all": "npm run docs:ts",
    "docs:ts": "typedoc --options typedoc.json --exclude *.spec.ts ./src/",
    "test": "karma start ./karma.config.js --single-run=true",
    "test:debug": "set KARMA_DEBUG=true && karma start ./karma.config.js --single-run=false",
    "test:tdd": "karma start ./karma.config.js --auto-watch --no-single-run",
    "test:e2e:wb:start": ".\\node_modules\\.bin\\webdriver-manager start",
    "test:e2e": ".\\node_modules\\.bin\\protractor .\\protractor.config.js",
    "generator": "node generator.js"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run test"
    }
  },
  "dependencies": {
    "@angular/router": "*",
    "LogUnobtrusiveExtension": "git+https://github.com/lwhiteley/AngularLogExtender.git#0.0.10",
    "angular": "^1.5.0",
    "angular-animate": "^1.5.0",
    "angular-aria": "^1.5.0",
    "angular-messages": "^1.5.0",
    "angular-ui-bootstrap": "^1.3.3",
    "bootstrap": "^3.3.6",
    "font-awesome": "^4.6.3",
    "jquery": "^2.2.4",
    "lodash": "^4.2.1",
    "ng-stats": "^2.5.2"
  },
  "devDependencies": {
    "angular": "^1.5.8",
    "angular-mocks": "^1.5.8",
    "autoprefixer": "^6.3.3",
    "chai": "^3.5.0",
    "chai-as-promised": "^5.3.0",
    "css-loader": "^0.23.1",
    "ejs-loader": "^0.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "ghooks": "^1.0.3",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.5.0",
    "json-server": "^0.8.10",
    "karma": "^1.1.1",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.1",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^0.3.7",
    "karma-jasmine-matchers": "^2.0.1",
    "karma-mocha": "^1.1.1",
    "karma-phantomjs-launcher": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "mkdirp": "^0.5.1",
    "mocha": "^2.5.3",
    "ng-annotate-loader": "^0.1.1",
    "opener": "^1.4.1",
    "phantomjs-polyfill": "0.0.1",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.8.1",
    "precss": "^1.4.0",
    "protractor": "^3.0.0",
    "rimraf": "^2.5.3",
    "string": "^3.3.1",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.2",
    "tslint": "^3.3.0",
    "tslint-loader": "^2.1.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "vorpal": "^1.10.8",
    "webdriver-manager": "^10.2.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
